<mapper namespace="com.jusun.web.mappers.PokemonMapper">
    <select id="selectAll" resultType="com.jusun.web.domins.PokemonDTO">
    select
    iname name,
    stamina stamina,
    atk atk,
    def def,
    capture_rate captureRate,
    flee_rate fleeRate,
    spawn_chance spawnChance,
    prima prim,
    secondary secondary,
    cp cp,
    url url
    from  pokemon
    </select>
</mapper>